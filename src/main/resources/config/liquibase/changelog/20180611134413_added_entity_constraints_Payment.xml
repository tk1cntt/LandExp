<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Payment.
    -->
    <changeSet id="20180611134413-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="house_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment_house_id"
                                 referencedColumnNames="id"
                                 referencedTableName="house"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="create_by_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment_create_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="update_by_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment_update_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
