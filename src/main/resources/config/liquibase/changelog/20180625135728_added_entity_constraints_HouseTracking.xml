<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity HouseTracking.
    -->
    <changeSet id="20180625135728-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="house_id"
                                 baseTableName="house_tracking"
                                 constraintName="fk_house_tracking_house_id"
                                 referencedColumnNames="id"
                                 referencedTableName="house"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="house_tracking"
                                 constraintName="fk_house_tracking_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
